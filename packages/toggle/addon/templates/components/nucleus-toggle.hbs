<label for="nucleus-toggle" role="checkbox" tabindex="0" aria-checked={{checkedValue}}>
  <input id="nucleus-toggle" type="checkbox" checked={{checkedValue}} onClick={{action "onValueChange" value="target.checked"}}>
  <span class="slider">
    <span class="circle">
      {{!-- {{svg-jar (if checkedValue 'nucleus-tick' 'nucleus-cross') aria-hidden=true role="img"}} --}}
    </span>
  </span>
</label>
