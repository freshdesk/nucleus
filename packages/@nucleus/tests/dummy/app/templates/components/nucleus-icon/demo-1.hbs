{{#docs-demo as |demo|}}
  {{#demo.example name="nucleus-icon.hbs"}}
    <div class="icon-container">
      {{#each icons as |icon|}}
        <div class="icon" aria-role="button" onClick={{action 'copyIcon' icon}}>
           {{nucleus-icon name=icon size="large" variant="none"}}
           <div class="icon--text">{{icon}}</div>
        </div>
      {{/each}}
    </div>
  {{/demo.example}}
{{/docs-demo}}